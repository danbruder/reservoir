{
  "name": "resevoir",
  "description": "A declarative clientside cache for Drupal 8 REST",
  "author": "Dan Bruder <dbruder@rapiddg.com> (http://github.com/dbruder)",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danbruder/resevoir.git"
  },
  "homepage": "https://github.com/danbruder/resevoir",
  "keywords": [
    "react",
    "redux",
    "drupal",
    "rest",
    "babel",
    "npm"
  ],
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0,react --plugins transform-runtime src/ --out-dir lib/",
    "dev": "./bin/local-dev.sh",
    "lint": "eslint -c .eslintrc src bin config",
    "test": "karma start --single-run",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-plugin-typecheck": "3.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.1",
    "eslint-loader": "1.0.0",
    "eslint-plugin-import": "0.12.1",
    "eslint-plugin-react": "3.7.1"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "karma": "0.13.19",
    "karma-cli": "0.1.2",
    "karma-mocha": "0.2.1",
    "karma-mocha-reporter": "1.1.5",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4"
  },
  "engines": {
    "node": ">=4.1.1",
    "npm": ">=3.3.12"
  },
  "bin": {
    "resevoir-dev": "./bin/local-dev.sh"
  },
  "bugs": {
    "url": "https://github.com/danbruder/resevoir/issues"
  }
}
